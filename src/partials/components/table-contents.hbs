
<!-- TABLE OF CONTENT -->

<!-- widget w-table-contents -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="widget w-table-contents set-max-width">
    <tr>
        <!-- widget-title-block -->
        <td class="widget-title-block no-padding-top" responsive="mobile-padding-horizontal-sides">
            <!-- widget-main-title -->
            <h2 class="widget-main-title">Table of Contents</h2>
            {{!-- <!-- widget-type-name -->
            <h4 class="widget-type-name"></h4> --}}
        </td>
    </tr>
    {{!--  --}}
    {{#forEach table_contents.list }}
    <!-- widget-content -->
    <tr class="widget-content">
        <td responsive="mobile-padding-horizontal-sides">
        <!-- content-link -->
            <p class="content-link"><strong><a href="{{ this.link }}" target="_blank">{{{ this.title }}}</a></strong></p>
            <!-- content-link-desc -->
            <p class="content-link-desc">{{{ this.description }}}</p>
        </td>
    </tr>
    {{/forEach}}
</table>