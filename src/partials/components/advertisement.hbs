                <a href="{{ advertisements.ussc.ad1.link }}" target="_blank"><img src="{{ default.image_dir_src }}/{{ advertisements.ussc.ad1.image }}" alt="{{ advertisements.ussc.ad1.alt }}" border="0" /></a>
