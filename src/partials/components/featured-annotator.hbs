
<!-- FEATURED ANNOTATOR -->

<!-- w-people-list w-featured-annotator -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-people-list w-featured-annotator set-max-width">
    <tr>
        <!-- list-table -->
        <td class="list-table" responsive="mobile-no-padding-top mobile-no-padding-bottom mobile-no-padding-horizontal-sides">

            <table border="0" cellpadding="0" cellspacing="0">
                <tr>
                    <!-- list-table-title -->
                    <td class="list-table-title" responsive="mobile-padding-horizontal-sides">
                        <!-- widget-type-name -->
                        <h4 class="widget-type-name">JUSTIA ANNOTATIONS</h4>
                        <!-- widget-main-title -->
                        <h2 class="widget-main-title"><a href="http://law.justia.com/annotations/" target="_blank">Featured Annotator</a></h2>
                    </td>
                </tr>
                <tr>
                    <td>
                        <!-- people-table -->
                        <table border="0" cellpadding="0" cellspacing="0" class="people-table">
                            <tr>
                                <!-- people-photo -->
                                <td class="people-photo" responsive="mobile-padding-left">
                                    <a href="[atty-link-1]" target="_blank"><img src="{{default.image_dir_src}}/annotator-photo-01.png" alt="[atty-name-1]" border="0" /></a>
                                </td>
                                <!-- people-info -->
                                <td class="people-info" responsive="mobile-padding-horizontal-sides">
                                    <!-- -name -->
                                    <p class="-name" responsive="mobile-fz-20"><a href="[atty-link-1]" target="_blank">[atty-name-1]</a></p>
                                    <!-- -location -->
                                    <p class="-location"><strong>[atty-location-1]</strong></p>
                                </td>
                                <!-- people-ranking -->
                                <td class="people-ranking" responsive="mobile-padding-right">
                                    <img src="{{default.image_dir_src}}/number-ranking-01-orange.png" alt="1" border="0" />
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>