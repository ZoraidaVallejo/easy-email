<!-- BLOCK STEPS -->

<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-block-steps set-max-width">
    <tr>
        <td class="head">
            <p><strong><a href="https://lawyers.justia.com/premium-placement" target="_blank">{{ title }}</a></strong></p>
        </td>
    </tr>
    <tr>
        {{> background classes="step-counter" image="welcome-step-separator-01.png" bgcolor="#06357a" dimensions="width:600px; height:21px;" bgStart=true content=true }}
            <table align="center" cellpadding="0" cellspacing="0" border="0">
                <tr>
                    <td>
                        <p><a href="https://lawyers.justia.com/premium-placement" target="_blank">{{ step }}</a></p>
                    </td>
                </tr>
            </table>
        {{> background bgEnd=true }}
    </tr>
    <tr>
        <td class="bottom-space">&nbsp;</td>
    </tr>
</table>
