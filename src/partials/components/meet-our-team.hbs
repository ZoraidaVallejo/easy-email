
<!-- MEET OUR TEAM -->

<!-- w-text-image w-meet-our-team -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-text-image w-meet-our-team set-max-width">
    <tr>
        <!-- item-list -->
        <td class="item-list" responsive="mobile-padding-horizontal-sides">
            <!-- item-1 -->
            <table cellpadding="0" cellspacing="0" border="0" class="item-1">
                <tr>
                    <!-- item-desc -->
                    <td class="item-desc" responsive="collapse-one">
                        <!-- widget-type-name -->
                        <h4 class="widget-type-name">MEET OUR TEAM</h4>
                        <!-- widget-main-title -->
                        <h2 class="widget-main-title">[meet-our-team-title]</h2>
                        <p>[meet-our-team-description]</p>
                    </td>
                    <!-- item-image -->
                    <td class="item-image" responsive="mobile-hide collapse-one">
                        <img src="{{default.image_dir_src}}/meet-our-team-01.jpg" alt="[meet-our-team-title]" border="0" />
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>