
<!-- DID YOU KNOW? -->

<!-- w-did-you-know -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-did-you-know set-max-width">
    <tr>
        <!-- dyk-title -->
        <td class="dyk-title" responsive="mobile-padding-horizontal-sides">
            <!-- widget-type-name -->
            <h4 class="widget-type-name">DID YOU KNOW?</h4>
            <!-- widget-main-title -->
            <h2 class="widget-main-title"><a href="{{ did_you_know.link }}" target="_blank">{{{ did_you_know.title }}}</a></h2>
        </td>
    </tr>
    <tr>
        <!-- dyk-desc -->
        <td class="dyk-desc" responsive="mobile-padding-horizontal-sides">
            <p>{{{ did_you_know.description }}}</p>
        </td>
    </tr>
    {{#is did_you_know.extra "" }}{{else}}
    <tr>
        <!-- dyk-link -->
        <td class="dyk-link" responsive="mobile-padding-horizontal-sides">
            <p>{{{ did_you_know.extra }}}</p>
        </td>
    </tr>
    {{/is}}
</table>