
<!-- DID YOU KNOW? -->

<!-- w-did-you-know -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-did-you-know set-max-width">
    <tr>
        <!-- dyk-title -->
        <td class="dyk-title" responsive="mobile-padding-horizontal-sides">
            <!-- widget-type-name -->
            <h4 class="widget-type-name">DID YOU KNOW?</h4>
            {{#if did_you_know.title }}
                <!-- widget-main-title -->
                <h2 class="widget-main-title"><a href="{{ did_you_know.link }}" target="_blank">{{{ did_you_know.title }}}</a></h2>
            {{/if}}
        </td>
    </tr>

    {{#each did_you_know.description }}
        <tr>
            <!-- dyk-desc {{#if @last }}dyk-desc--last{{/if}} -->
            <td class="dyk-desc {{#if @last }}dyk-desc--last{{/if}}" responsive="mobile-padding-horizontal-sides">
                <p>{{{ this }}}</p>
            </td>
        </tr>
    {{/each}}
    <tr>
        <!-- button -->
        <td class="button" responsive="mobile-padding-horizontal-sides">
            {{> button visitSite=true buttonLink=did_you_know.link }}
        </td>
    </tr>
</table>
