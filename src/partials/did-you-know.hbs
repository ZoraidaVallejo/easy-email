
<!-- DID YOU KNOW? -->
{{#if jldNewsletter }}
<!-- w-did-you-know -jld -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-did-you-know -jld set-max-width">
{{/if}}
{{#if clientsNewsletter }}
<!-- w-did-you-know -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-did-you-know set-max-width">
{{/if}}
  <tr>
    <!-- dyk-title -->
    <td class="dyk-title" responsive="mobile-padding-horizontal-sides">
      <!-- widget-type-name -->
      <h4 class="widget-type-name">DID YOU KNOW?</h4>
      {{#if clientsNewsletter }}
        {{#if didYouKnow.clients.title }}
          <!-- widget-main-title -->
          <h2 class="widget-main-title"><a href="{{ didYouKnow.clients.link }}" target="_blank">{{{ didYouKnow.clients.title }}}</a></h2>
        {{/if}}
      {{/if}}
      {{#if jldNewsletter }}
        {{#if didYouKnow.jld.title }}
          <!-- widget-main-title -->
          <h2 class="widget-main-title"><a href="{{ didYouKnow.jld.link }}" target="_blank">{{{ didYouKnow.jld.title }}}</a></h2>
        {{/if}}
      {{/if}}
    </td>
  </tr>
  {{#if clientsNewsletter }}
    {{#each didYouKnow.clients.description }}
      <tr>
        <!-- dyk-desc {{#if @last }}dyk-desc--last{{/if}} -->
        <td class="dyk-desc {{#if @last }}dyk-desc--last{{/if}}" responsive="mobile-padding-horizontal-sides">
          <p>{{{ this }}}</p>
        </td>
      </tr>
    {{/each}}
  {{/if}}
  {{#if jldNewsletter }}
    {{#each didYouKnow.jld.description }}
      <tr>
        <!-- dyk-desc {{#if @last }}dyk-desc--last{{/if}} -->
        <td class="dyk-desc" responsive="mobile-padding-horizontal-sides">
          <p>{{{ this }}}</p>
        </td>
      </tr>
    {{/each}}
    <tr>
      <!-- button-didYouKnow -->
      <td class="button-did-you-know dyk-desc--last" responsive="mobile-padding-horizontal-sides">
        {{> ui-real-text-button buttonText='Read More' buttonLink=didYouKnow.jld.link }}
      </td>
    </tr>
  {{/if}}
</table>
