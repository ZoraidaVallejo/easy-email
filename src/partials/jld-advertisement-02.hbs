
<!-- JLD ADVERTISEMENT -->

<!-- w-advertisement w-jld-ad-02 -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-advertisement w-jld-ad-02 set-max-width">
  <tr>
    <!-- ad-justia-logo -->
    <td class="ad-justia-logo" responsive="mobile-padding-top mobile-padding-bottom mobile-padding-horizontal-sides">
      <a href="{{{ advertisements.jld.ad2.link }}}" target="_blank">
        <img src="{{ default.imageDirSrc }}/justia-marketing-logo.png" alt="JUSTIA Marketing" border="0" />
      </a>
    </td>
  </tr>
  <tr>
    <!-- ad-title -->
    <td class="ad-title" responsive="mobile-padding-bottom mobile-padding-horizontal-sides">
      <a href="{{{ advertisements.jld.ad2.link }}}" target="_blank">
        <strong responsive="mobile-fz-20 mobile-align-center">{{{ advertisements.jld.ad2.title }}}</strong>
      </a>
    </td>
  </tr>

  <tr>
    <!-- ad-desc -->
    <td class="ad-desc-01" responsive="mobile-padding-top mobile-padding-bottom mobile-padding-horizontal-sides">
      <p>
        <a href="{{{ advertisements.jld.ad2.link }}}" target="_blank">
          {{{ advertisements.jld.ad2.description-01 }}}
        </a>
      </p>
    </td>
  </tr>

  <tr>
    <!-- ad-image -->
    <td class="ad-image" responsive="mobile-hide">
      <a href="{{{ advertisements.jld.ad2.link }}}" target="_blank">
        <img src="{{ default.imageDirSrc }}/{{ advertisements.jld.ad2.image }}" alt="{{ advertisements.jld.ad2.alt }}" border="0" />
      </a>
    </td>
  </tr>

  <tr>
    <!-- ad-desc-02 -->
    <td class="ad-desc-02" responsive="mobile-no-padding-top mobile-padding-bottom mobile-padding-horizontal-sides">
      <p responsive="mobile-fz-16">
        <a href="{{{ advertisements.jld.ad2.link }}}" target="_blank">
          {{{ advertisements.jld.ad2.description-02 }}}
        </a>
      </p>
    </td>
  </tr>

  <tr>
    <td class="ad-footer" responsive="mobile-no-padding-top mobile-padding-bottom">
      <table align="center" border="0" cellpadding="0" cellspacing="0">
        <tr>
          <td class="ad-button -red -large" responsive="mobile-full-width mobile-no-padding-horizontal-sides mobile-align-center mobile-padding-top mobile-padding-bottom">
            {{> ui-real-text-button responsiveClasses="mobile-align-center mobile-reset-width mobile-padding-horizontal-sides mobile-no-float mobile-fz-14" buttonLink=advertisements.jld.ad2.link buttonText=advertisements.jld.ad2.button.text }} 
          </td>  
          <td class="ad-phone" responsive="mobile-full-width">
            <p responsive="mobile-fz-14">
              {{{ advertisements.jld.ad2.phone }}}
            </p>
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>
