
<!-- ADVERTISEMENT -->

<!-- w-advertisement w-client-ad-01 -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-advertisement w-client-ad-01 set-max-width">
  <tr>
    <!-- ad-image -->
    <td class="ad-image">
      <a href="{{{ advertisements.clients.ad1.link-a }}}">
        <img src="{{ default.imageDirSrc }}/{{ advertisements.clients.ad1.image-a }}" alt="{{ advertisements.clients.ad1.alt-a }}" border="0" />
      </a>
    </td>
  </tr>
  {{#if advertisements.clients.ad1.description }}
    {{#each advertisements.clients.ad1.description }}
      <tr>
        <!-- ad-desc {{#if @first }}ad-desc--first{{/if}} {{#if @last }}ad-desc--last{{/if}} -->
        <td class="ad-desc {{#if @first }}ad-desc--first{{/if}} {{#if @last }}ad-desc--last{{/if}}" responsive="mobile-padding-top mobile-padding-horizontal-sides">
          <p><a href="{{{ ../advertisements.clients.ad1.link }}}">{{{ this }}}</a></p>
        </td>
      </tr>
    {{/each}}
  {{/if}}
  <tr>
    <!-- ad-image -->
    <td class="ad-image">
      <a href="{{{ advertisements.clients.ad1.link }}}">
        <img src="{{ default.imageDirSrc }}/{{ advertisements.clients.ad1.image-b }}" alt="{{ advertisements.clients.ad1.alt-b }}" border="0" />
      </a>
    </td>
  </tr>
</table>
