
<!-- ADVERTISEMENT -->

<!-- w-advertisement w-client-ad-01 -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-advertisement set-max-width">
    <tr>
        <!-- ad-image -->
        {{> ui-background classes="ad-image" image="jobs-premium-01.png" bgColor="#cde2e8" dimensions="width:600px; height:180px;" bgStart=true content=true }}
          <table align="center" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <!-- ad-content -->
              <td class="ad-content">
                <p class="pt-1">Lawyers: Want to view your traffic stats?</p>
                <p class="pt-2"><strong>Become a Premium&nbsp;<br>Member Now!</strong></p>
              </td>
            </tr>
            <tr>
                <td class="ad-button">
                    {{> ui-real-text-button buttonLink=this.link buttonText="Click Here For More Info" }}
                </td>
            </tr>
          </table>
        {{> ui-background bgEnd=true }}
    </tr>
</table>
