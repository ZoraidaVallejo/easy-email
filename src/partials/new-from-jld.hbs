
<!-- NEW FROM JUSTIA LAWYER DIRECTORY -->

<!-- w-new-from-jld -->
<table align="center" border="0" cellpadding="0" cellspacing="0" class="w-new-from-jld set-max-width">
  <tr>
    <!-- nfjld-title -->
    <td class="nfjld-title" responsive="mobile-padding-horizontal-sides">
      <!-- widget-type-name -->
      <h4 class="widget-type-name">NEW FROM JUSTIA LAWYER DIRECTORY</h4>
      {{#if newFromJld.title }}
        <!-- widget-main-title -->
        <h2 class="widget-main-title"><a href="{{ newFromJld.link }}" target="_blank">{{{ newFromJld.title }}}</a></h2>
      {{/if}}
    </td>
  </tr>

  {{#each newFromJld.description }}
    <tr>
      <!-- nfjld-desc -->
      <td class="nfjld-desc" responsive="mobile-padding-horizontal-sides">
        <p>{{{ this }}}</p>
      </td>
    </tr>
  {{/each}}
  <tr>
    <!-- nfjld-desc nfjld-desc--last -->
    <td class="nfjld-desc nfjld-desc--last" responsive="mobile-padding-horizontal-sides">
      {{> ui-real-text-button buttonLink=newFromJld.button.link buttonText=newFromJld.button.text }}
    </td>
  </tr>
</table>
