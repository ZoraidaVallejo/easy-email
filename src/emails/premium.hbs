---
subject: Your Justia Lawyer Directory Monthly Snapshot
---

{{>preheader "Take a look at your report."}}

<table border="0" cellpadding="0" cellspacing="0" width="100%" class="jld-newsletter">

  {{!-- HEADER --}}
  <tr>
    <!-- align-center valign-top -->
    <td height="100%" width="100%" class="align-center valign-top">
      {{>main-header
        logoName=default.header.logo.name
        logoLink=default.header.logo.link.url
      }}
    </td>
  </tr>



  {{!-- CONTENT --}}
  <tr>
    <!-- align-center valign-top -->
    <td height="100%" width="100%" class="align-center valign-top">
      {{>ui-layout-hack tableStart=true}}
        <!-- top-container-wrapper -->
        <table align="center" border="0" cellpadding="0" cellspacing="0" class="top-container-wrapper set-max-width">
          <tr>
            {{> ui-background
              classes="blocks-wrapper"
              responsiveClasses="mobile-padding-horizontal-sides"
              image="fade-background-01.png"
              bgColor="#f2f2f2"
              dimensions="width:600px;"
              bgStart=true
              content=true
            }}
              {{>welcome premium=true}}
              {{>widget-gap}}
              {{>profile-info}}
              {{>widget-gap}}
              {{>profile-stats premium=true}}
              {{>widget-gap}}
              {{>rating}}
              {{>widget-gap}}
              {{>contact-info}}
              {{>widget-gap}}
              {{>ad}}
            {{>ui-background bgEnd=true}}
          </tr>
        </table>
      {{>ui-layout-hack tableEnd=true}}
    </td>
  </tr>

  {{!-- FOOTER --}}
  <tr>
    <!-- align-center valign-top -->
    <td height="100%" width="100%" class="align-center valign-top">
      {{>main-footer
        navigationElements=default.footer.navigation
        disclaimerText=default.footer.disclaimer
        addressElements=default.footer.address
      }}
    </td>
  </tr>
</table>
