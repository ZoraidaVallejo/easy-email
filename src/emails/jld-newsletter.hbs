---
layout: responsive.hbs
subject: <%= currentMonthString %> <%= currentYear %> edition - Justiaâ€™s exclusive newsletter!
---

{{>preheader text="News and developments from Justia and the tech world ----- "}}

{{#table class="jld-newsletter" align=""}}
  <tr>
    <td class="full-size align-center valign-top">
      {{>main-header
        withDate=true
        logoName="insights"
        logoLink=default.header.logo.link.jld.url
        logoUtmVars=default.header.logo.link.jld.utm
      }}
    </td>
  </tr>
  <tr>
    <td class="full-size align-center valign-top">
      {{>ui-layout-hack tableStart=true}}
        {{>welcome widgetConfig=welcome.jld}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>featured-news widgetUtmVars=featuredNews.utm.jld}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>tech-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>seo-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>did-you-know didYouKnowConfig=didYouKnow.jld}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>social-media-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>legal-industry-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>tip-month tipMonthConfig=tipMonth.jld}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>jobs-newsletter}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>jurispro}}

      {{>ui-layout-hack tableEnd=true}}
    </td>
  </tr>
  <tr>
    <td class="full-size align-center valign-top">
      {{>main-footer
        socialOnTop=true
        lookAndFeel="light-bright"
        logoName="insights"
        logoLink=default.footer.logo.link.jld.url
        logoUtmVars=default.footer.logo.link.jld.utm
        navigationElements=default.footer.navigation.items
        disclaimerText=default.footer.disclaimer.jld
        addressElements=default.footer.address
      }}
    </td>
  </tr>
{{/table}}
