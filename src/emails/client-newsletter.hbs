---
layout: responsive-opensans.hbs
subject: <%= currentMonthString %> <%= currentYear %> edition - Justiaâ€™s exclusive client newsletter!
---

{{>preheader text="News and developments from Justia and the tech world ----- "}}

<!-- client-newsletter -->
<table border="0" cellpadding="0" cellspacing="0" width="100%" class="client-newsletter">

  {{!-- HEADER --}}
  <tr>
    <!-- align-center valign-top -->
    <td height="100%" width="100%" class="align-center valign-top">
      {{>main-header
        logoName='lawyer-directory'
        logoLink=default.header.logo.link.clients.url
      }}
    </td>
  </tr>

  {{!-- CONTENT --}}
  <tr>
    <!-- align-center valign-top -->
    <td height="100%" width="100%" class="align-center valign-top">

      {{>ui-layout-hack tableStart=true}}
        {{>welcome clientsNewsletter=true}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>featured-news clientsNewsletter=true}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>tip-month clientsNewsletter=true}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>tech-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>internet-stats}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>seo-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>featured-site-month singleSite=true}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>in-the-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>inside-justia}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>did-you-know}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>meet-our-team}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>social-media-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>advertisement}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>pugs-tale}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>legal-industry-news}}

      {{>ui-layout-hack tableMiddleSeparator=true}}
        {{>law-events}}

      {{>ui-layout-hack tableEnd=true}}

    </td>
  </tr>



  {{!-- FOOTER --}}
  <tr>
    <!-- align-center valign-top -->
    <td height="100%" width="100%" class="align-center valign-top">
      {{>main-footer
        navigationElements=default.footer.navigation.items
        disclaimerText=default.footer.disclaimer.clients
        addressElements=default.footer.address.clients
      }}
    </td>
  </tr>

</table>
