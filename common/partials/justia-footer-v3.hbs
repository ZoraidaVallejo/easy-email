{{> ui-layout-hack tableStart=true }}
  <!-- justia-footer-v3 set-max-width -->
  <table align="center" border="0" cellpadding="0" cellspacing="0" class="justia-footer-v3 set-max-width">
    <tr>
      <!-- logo-footernav -->
      <td class="logo-footernav" responsive="mobile-padding-horizontal-sides">
        <!-- spacer -->
        <img src="https://justatic.com/v/1/emails/images/spacer.gif" class="spacer" border="0" responsive="mobile-hide">
        <table align="center" border="0" cellpadding="0" cellspacing="0" class="set-max-width">
          <tr>
            <!-- logo -->
            <td class="logo" responsive="collapse-one mobile-padding-bottom">
              <img src="{{ default.footer.logo.v3.url }}" alt="{{ default.footer.logo.v3.alt }}" border="0" responsive="mobile-align-center" />
            </td>
            <!-- footernav -->
            <td class="footernav" responsive="collapse-one mobile-align-center">
              <p>
                <a href="https://www.justia.com/marketing/" target="_blank" responsive="collapse-one mobile-padding-vertical-sides">Contact Us</a>
                <!-- vline -->
                <span class="vline" responsive="mobile-hide">|</span>
                <a href="https://www.justia.com/marketing/privacy-policy/" target="_blank" responsive="collapse-one mobile-padding-vertical-sides">Privacy Policy</a>
                <!-- vline -->
                <span class="vline" responsive="mobile-hide">|</span>
                <a href="{{{ default.unsubscribeUrl }}}" responsive="collapse-one mobile-padding-vertical-sides">Unsubscribe</a>
              </p>
            </td>
          </tr>
        </table>
      </td>
    </tr>
    <tr>
      <!-- social -->
      <td class="social" responsive="mobile-padding-horizontal-sides">
        <!-- spacer -->
        <img src="https://justatic.com/v/1/emails/images/spacer.gif" class="spacer" border="0" responsive="mobile-hide">
        <table align="center" border="0" cellpadding="0" cellspacing="0" class="set-max-width">
          <tr>
            <td><a href="https://www.facebook.com/justia" target="_blank"><img src="https://lawyers.justia.com/s/facebook-flat-64.png" alt="Facebook" border="0" /></a></td>
            <td><a href="https://twitter.com/justiacom" target="_blank"><img src="https://lawyers.justia.com/s/twitter-flat-64.png" alt="Twitter" border="0" /></a></td>
            <td><a href="https://www.linkedin.com/company/justia" target="_blank"><img src="https://lawyers.justia.com/s/linkedin-flat-64.png" alt="LinkedIn" border="0" /></a></td>
            <td><a href="https://lawyers.justia.com/organization/justia-inc-16302" target="_blank"><img src="https://lawyers.justia.com/s/justia-flat-64.png" alt="Justia" border="0" /></a></td>
          </tr>
        </table>
      </td>
    </tr>
    <tr>
      <!-- disclaimer -->
      <td class="disclaimer" responsive="mobile-padding-horizontal-sides">
        <p>You are receiving this email because you are in the lawyer directory.</p>
      </td>
    </tr>
    <tr>
      <!-- address -->
      <td class="address" responsive="mobile-padding-horizontal-sides">
        <p>
          <!-- justia-inline --><a class="justia-inline" href="https://www.justia.com/">Justia</a> <!-- vline --><span class="vline">|</span> <span id="ios-links-black">1380 Pear Ave #2B, Mountain View, CA 94043 {{!-- <!-- vline --><span class="vline">|</span> Tel: (888) 587-8421</span> --}}
        </p>
      </td>
    </tr>
  </table>
{{> ui-layout-hack tableEnd=true }}
