{{!
  Notification Block
  Params:
    date:   (string) unescaped Date of the current newslatter
}}
{{> ui-layout-hack tableStart=true }}
  <!-- justia-header-v3 -->
  <table align="center" border="0" cellpadding="0" cellspacing="0" class="justia-header-v3 set-max-width">
    <tr>
      <!-- logo -->
      <td class="logo" responsive="mobile-padding-horizontal-sides">
        <!-- spacer -->
        <img src="https://justatic.com/v/1/emails/images/spacer.gif" class="spacer" responsive="mobile-hide" border="0" />
        <a href="{{ default.header.logo.v2.url }}" target="_blank">
          <!-- logo-img -->
          <img src="{{ default.header.logo.v2.source }}" alt="{{ default.header.logo.v2.alt }}" border="0" class="logo-img" />
        </a>
      </td>
    </tr>
  </table>
{{> ui-layout-hack tableEnd=true }}
