{{!
    Notification Block
    Params:
        date:   (string) unescaped Date of the current newslatter
}}
{{> fixedLayoutHack tableStart=true }}
    <!-- main-header-v3 set-max-width -->
    <table align="center" border="0" cellpadding="0" cellspacing="0" class="main-header-v3 set-max-width">
        <tr>
            <!-- view-in-browser align-center -->
            <td class="view-in-browser align-center" responsive="mobile-padding-horizontal-sides">
                <p>If you are unable to see this message, <a href="" target="_blank">click here</a> to view it in a web browser.</p>
            </td>
        </tr>
        <tr>
            <!-- logo -->
            <td class="logo" responsive="mobile-padding-horizontal-sides">
                <a href="{{ default.header.logo.v3.url }}" target="_blank">
                    <!-- logo-img  -->
                    <img src="{{ default.header.logo.v3.source }}" alt="{{ default.header.logo.v3.alt }}" border="0" class="logo-img" />
                </a>
            </td>
        </tr>
        <tr>
            <!-- subtitle -->
            <td class="subtitle" responsive="mobile-padding-horizontal-sides">
                <p responsive="mobile-fz-16"><strong>US Supreme Court</strong></p>
            </td>
        </tr>
    </table>
{{> fixedLayoutHack tableEnd=true }}
